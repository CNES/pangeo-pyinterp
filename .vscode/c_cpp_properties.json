{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/anaconda3/include/",
                "~/anaconda3/include/eigen3/",
                "~/anaconda3/include/python3.7m"
            ],
            "defines": [
                "HAVE_IEC559=1"
            ]],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "MacOSX",
            "includePath": [
                "${workspaceFolder}/**",
                "~/opt/miniconda3/include/",
                "~/opt/miniconda3/include/eigen3/",
                "~/opt/miniconda3/include/python3.8"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "Windows",
            "includePath": [
                "${workspaceFolder}/**",
                "~\\Anaconda3\\include",
                "~\\Anaconda3\\Library\\include",
                "~\\Anaconda3\\Library\\include\\eigen3",
                "C:\\Program Files (x86)\\Microsoft Visual Studio\\2017\\Community\\VC\\Tools\\MSVC\\14.16.27023\\include",
                "${workspaceFolder}/third_party/pybind11/include"
            ],
            "defines": [],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "msvc-x64"
        }
