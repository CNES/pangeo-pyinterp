import enum

class BoundaryCheck(enum.Enum):
    CONVEX_HULL = ...
    ENVELOPE = ...
    NONE = ...

class CovarianceFunction(enum.Enum):
    CAUCHY = ...
    GAUSSIAN = ...
    MATERN_12 = ...
    MATERN_32 = ...
    MATERN_52 = ...
    SPHERICAL = ...
    WENDLAND = ...

class DriftFunction(enum.Enum):
