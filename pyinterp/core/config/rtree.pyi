    QUADRATIC = ...

class InterpolationWindow:
    def __init__(self) -> None: ...
    def with_arg(self, value: float | None) -> InterpolationWindow: ...
    def with_boundary_check(
        self, value: BoundaryCheck
    ) -> InterpolationWindow: ...
    def with_k(self, value: int) -> InterpolationWindow: ...
    def with_num_threads(self, value: int) -> InterpolationWindow: ...
    def with_radius(self, value: float | None) -> InterpolationWindow: ...
    def with_wf(self, value: WindowKernel) -> InterpolationWindow: ...

